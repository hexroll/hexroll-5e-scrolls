    ProficiencyBonus = 5
    HitPoints @ 13d8+8
    BaseStrength @ 3d6
    BaseDexterity @ 3d6
    BaseConstitution @ 5d3+6
    BaseIntelligence @ 3d6
    BaseWisdom @ 5d3+6
    BaseCharisma @ 3d6

    CantripsKnown = 4
    Level1Slots = 4
    Level2Slots = 3
    Level3Slots = 3
    Level4Slots = 3
    Level5Slots = 2
    Level6Slots = 1
    Level7Slots = 1
    Level8Slots = 0
    Level9Slots = 0

    WildShape @ [
        * Beast Shape - Brown Bear
        * Beast Shape - Dire Wolf
        * Beast Shape - Eagle
        * Beast Shape - Giant Eagle
        * Beast Shape - Flying Snake
        * Beast Shape - Giant Octopus
        * Beast Shape - Giant Hyena
        * Beast Shape - Giant Spider
        * Beast Shape - Giant Toad
        * Beast Shape - Giant Vulture
        * Beast Shape - Hawk
        * Beast Shape - Lion
        * Beast Shape - Panther
        * Beast Shape - Tiger
        * Beast Shape - Wolf
    ]
    DruidCircle @ DruidCircleLevel14
    | Druid
}

