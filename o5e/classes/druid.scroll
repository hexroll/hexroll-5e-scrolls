DruidCircle {
  Stats! ~ <%
    <li><strong>Extra Cantrip:</strong> {{ExtraCantrip}}</li>
    <li><strong>Natural Recovery:</strong> {{NaturalRecovery}}</li>
  %>
  Actions! ~ <%

    {% if exists("ExtraCircleSpellForLevel3") %}
    <li><strong>Circle Spells:</strong>
      {{ExtraCircleSpellForLevel3}}
      {% if exists("ExtraCircleSpellForLevel5") %}
      , {{ExtraCircleSpellForLevel5}}
      {% endif %}
      {% if exists("ExtraCircleSpellForLevel7") %}
      , {{ExtraCircleSpellForLevel7}}
      {% endif %}
      {% if exists("ExtraCircleSpellForLevel9") %}
      , {{ExtraCircleSpellForLevel9}}
      {% endif %}
    </li>
    {% endif %}
  %>

}

